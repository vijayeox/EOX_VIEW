{
  "name": "@oxzion/gui",
  "version": "1.0.0",
  "scripts": {
    "test": "npm run eslint && npm run stylelint",
    "eslint": "eslint index.js 'src/**/*.js'",
    "build": "webpack",
    "build:esm": "rollup -c",
    "watch": "webpack --watch",
    "watch:esm": "rollup -c --watch",
    "stylelint": "stylelint index.scss src/**/*.scss",
    "prepublishOnly": "npm run test && rm ./dist/* && NODE_ENV=production npm run build && npm run build:esm"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.13",
    "@amcharts/amcharts4-geodata": "^4.1.14",
    "@progress/kendo-data-query": "^1.5.3",
    "@progress/kendo-drawing": "^1.6.0",
    "@progress/kendo-react-animation": "^3.12.0",
    "@progress/kendo-react-buttons": "^3.12.0",
    "@progress/kendo-react-data-tools": "^3.12.0",
    "@progress/kendo-react-dateinputs": "^3.12.0",
    "@progress/kendo-react-dialogs": "^3.12.0",
    "@progress/kendo-react-dropdowns": "^3.12.0",
    "@progress/kendo-react-excel-export": "^2.10.0",
    "@progress/kendo-react-grid": "^3.12.0",
    "@progress/kendo-react-inputs": "^3.12.0",
    "@progress/kendo-react-intl": "^3.12.0",
    "@progress/kendo-react-popup": "^3.12.0",
    "@progress/kendo-react-upload": "^3.12.0",
    "@progress/kendo-theme-bootstrap": "^4.14.0",
    "@progress/kendo-theme-default": "^3.7.0",
    "@progress/kendo-react-editor": "^3.10.1",
    "@trendmicro/react-buttons": "^1.3.1",
    "@trendmicro/react-dropdown": "^1.4.0",
    "@trendmicro/react-sidenav": "^0.4.5",
    "ansi-colors": "^4.1.1",
    "bootstrap": "4.4.1",
    "bootswatch": "4.4.1",
    "deepmerge": "^4.2.2",
    "file-loader": "^4.3.0",
    "file-upload-with-preview": "^4.0.8",
    "formiojs": "^4.9.18",
    "history": "^4.10.1",
    "jquery": "^3.4.0",
    "libphonenumber-js": "^1.7.50",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "npm": "^6.14.4",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react-avatar-image-cropper": "^1.3.6",
    "react-bootstrap": "^1.0.0",
    "react-datepicker": "^2.14.1",
    "react-flippy": "^0.1.5",
    "react-formio": "^3.1.12",
    "react-jsx-parser": "^1.21.0",
    "react-loader-spinner": "^3.1.5",
    "react-mentions": "^3.3.1",
    "react-notifications-component": "^1.1.1",
    "react-select": "^3.1.0",
    "react-switch": "^5.0.1",
    "react-web-tabs": "^1.0.1",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "scroll-into-view-if-needed": "^2.2.24",
    "serve": "^11.3.0",
    "sweetalert2": "^9.10.9",
    "url-loader": "^2.3.0",
    "react-webcam": "^1.1.1",
    "react-phone-number-input": "^2.5.3",
    "viewerjs": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.12.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "peerDependencies": {
    "react-icons": "^3.6.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": "@osjs/eslint-config"
  },
  "esdoc": {
    "source": "./src",
    "destination": "./doc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "brand": {
            "title": "EOX GUI API",
            "description": "EOX GUI API Documentation"
          },
          "lint": {
            "enable": false
          },
          "coverage": {
            "enable": false
          },
          "undocumentIdentifier": {
            "enable": false
          }
        }
      },
      {
        "name": "esdoc-publish-html-plugin"
      }
    ]
  }
}
