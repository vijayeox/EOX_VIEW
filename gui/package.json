                {
                    "modules": false
                }
            ]
        ],
        "plugins": [
            "@babel/plugin-transform-runtime"
        ]
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "node": true
        },
        "parserOptions": {
            "sourceType": "module"
        },
        "extends": "@osjs/eslint-config"
    },
    "esdoc": {
        "source": "./src",
        "destination": "./doc",
        "plugins": [
            {
                "name": "esdoc-standard-plugin",
                "option": {
                    "brand": {
                        "title": "EOX GUI API",
                        "description": "EOX GUI API Documentation"
                    },
                    "lint": {
                        "enable": false
                    },
                    "coverage": {
                        "enable": false
                    },
                    "undocumentIdentifier": {
                        "enable": false
                    }
                }
            },
            {
                "name": "esdoc-publish-html-plugin"
            }
        ]
    },
    "osjs": {
        "type": "package"
    }
